{
  "name": "spotify-timeline",
  "version": "0.8.0",
  "description": "A web app that takes your Spotify account and creates a timeline of all your songs, when they were added and attempts to make playlists or 'eras' of your music listening history.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dillondrenzek/Spotify-Timeline.git"
  },
  "keywords": [
    "spotify",
    "music",
    "timeline"
  ],
  "author": "Dillon Drenzek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dillondrenzek/Spotify-Timeline/issues"
  },
  "homepage": "https://github.com/dillondrenzek/Spotify-Timeline#readme",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "jest",
    "dev": "nodemon --watch \".\" --ext \"ts\" --exec \"yarn start\""
  },
  "dependencies": {
    "axios": "0.21.2",
    "cookie-parser": "1.4.5",
    "dotenv": "8.2.0",
    "express": "4.17.3",
    "express-query-parser": "^1.3.3",
    "http": "0.0.1-security",
    "https": "1.0.0",
    "lodash": "^4.17.21",
    "luxon": "^2.5.2",
    "querystring": "^0.2.1",
    "simple-statistics": "^7.8.2"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.9",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.3.2",
    "@types/node": "14.0.24",
    "jest": "26.2.2",
    "nodemon": "^2.0.20",
    "ts-jest": "26.1.4",
    "tslint": "6.1.2",
    "typescript": "3.9.7"
  }
}
