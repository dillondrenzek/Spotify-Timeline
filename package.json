{
  "name": "spotify-timeline",
  "version": "0.2.1",
  "description": "A web app that takes your Spotify account and creates a timeline of all your songs, when they were added and attempts to make playlists or 'eras' of your music listening history.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dillondrenzek/Spotify-Timeline.git"
  },
  "keywords": [
    "spotify",
    "music",
    "timeline"
  ],
  "author": "Dillon Drenzek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dillondrenzek/Spotify-Timeline/issues"
  },
  "homepage": "https://github.com/dillondrenzek/Spotify-Timeline#readme",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run style:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "gulp": "gulp",
    "style:w": "gulp watch:styl",
    "serve": "tsc && concurrently \"npm run tsc:w\" \"npm run style:w\" \"npm run serve:dev\" ",
    "serve:dev": "node-dev index.js",
    "serve:prod": "node index.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",

    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.17",

    "angular2-in-memory-web-api": "0.0.18",
    "bootstrap": "^3.3.6",

    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "http": "0.0.0",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "concurrently": "^2.2.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.3.1",
    "gulp-watch": "^4.3.5",
    "lite-server": "^2.2.2",
    "node-dev": "^3.1.1",
    "typescript": "^2.0.2",
    "typings":"^1.3.2"
  }
}
