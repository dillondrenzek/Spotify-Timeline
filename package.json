{
  "name": "spotify-timeline",
  "version": "0.3.2",
  "description": "A web app that takes your Spotify account and creates a timeline of all your songs, when they were added and attempts to make playlists or 'eras' of your music listening history.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dillondrenzek/Spotify-Timeline.git"
  },
  "keywords": [
    "spotify",
    "music",
    "timeline"
  ],
  "author": "Dillon Drenzek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dillondrenzek/Spotify-Timeline/issues"
  },
  "homepage": "https://github.com/dillondrenzek/Spotify-Timeline#readme",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean ; npm run build ; npm run serve",
    "start:w": "npm run clean ; concurrently \"npm run build:watch\" \"npm run serve:dev\"",

    "serve": "npm run serve:dev",
    "serve:dev": "node-dev index.js",
    "serve:prod": "node index.js",

    "build": "concurrently \"npm run build:ts\" \"npm run build:styl\"",
    "prebuild:ts": "typings install",
    "build:ts": "tsc",
    "build:styl": "gulp styl",

    "build:watch": "concurrently \"npm run build:ts:watch\" \"npm run build:styl:watch\"",
    "prebuild:ts:watch": "typings install",
    "build:ts:watch": "tsc -w",
    "build:styl:watch": "gulp watch:styl",


    "clean": "gulp clean:all",
    "clean:js": "gulp clean:js",
    "clean:css": "gulp clean:css",
    "clean:typings": "gulp clean:typings"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",

    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23",

    "angular2-in-memory-web-api": "0.0.20",
    "bootstrap": "^3.3.6",

    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "http": "0.0.0",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "concurrently": "^2.2.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.3.1",
    "gulp-watch": "^4.3.5",
    "lite-server": "^2.2.2",
    "node-dev": "^3.1.1",
    "typescript": "^2.0.2",
    "typings":"^1.3.2"
  }
}
